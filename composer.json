{
  "name": "bueltge/marksimple",
  "description": "A simple Markdown parser for PHP.",
  "license": "MIT",
  "keywords": [
    "markdown",
    "parser",
    "php"
  ],
  "authors": [
    {
      "name": "Frank BÃ¼ltge",
      "email": "frank@bueltge.de"
    }
  ],
  "require": {
    "php": ">=7"
  },
  "require-dev": {
    "phpunit/phpunit": "6.3.*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.2",
    "squizlabs/php_codesniffer": "^3"
  },
  "autoload": {
    "psr-4": {
      "Bueltge\\Marksimple\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bueltge\\Marksimple\\": "src/",
      "Bueltge\\Marksimple\\Tests\\Unit\\": "tests/phpunit/Unit/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
