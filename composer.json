{
    "name": "bueltge/marksimple",
    "description": "A simple Markdown parser for PHP.",
    "license": "MIT",
    "keywords": [
        "markdown",
        "parser",
        "php"
    ],
    "authors": [
        {
            "name": "Frank BÃ¼ltge",
            "email": "frank@bueltge.de"
        }
    ],
    "require": {
        "php": ">=7",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "codedungeon/phpunit-result-printer": "^0.26",
        "inpsyde/php-coding-standards": "^0.13",
        "squizlabs/php_codesniffer": "~3.5.4",
        "antecedent/patchwork": "^2.1",
        "dg/composer-cleaner": "^2"
    },
    "autoload": {
        "psr-4": {
            "Bueltge\\Marksimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bueltge\\Marksimple\\Tests\\Unit\\": "tests/phpunit/Unit/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
        "scripts": {
        "cs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "qa": ["@cs"]
    }
}
