{
    "name": "bueltge/marksimple",
    "description": "A simple Markdown parser for PHP.",
    "license": "MIT",
    "keywords": [
        "markdown",
        "parser",
        "php"
    ],
    "authors": [
        {
            "name": "Frank BÃ¼ltge",
            "email": "frank@bueltge.de"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "inpsyde/php-coding-standards": "^0.13",
        "antecedent/patchwork": "^2.1",
        "dg/composer-cleaner": "^2"
    },
    "autoload": {
        "psr-4": {
            "Bueltge\\Marksimple\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bueltge\\Marksimple\\Tests\\Unit\\": "tests/phpunit/Unit/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
